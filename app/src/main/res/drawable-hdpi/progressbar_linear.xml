<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background">
        <shape>

            <corners
                android:radius="5dp"/>
            <gradient
                android:angle="225"
                android:startColor="#9FA9D8"
                android:endColor="#009FA9D8"/>
        </shape>
    </item>
    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <corners
                    android:radius="5dp"/>
                <solid android:color="#DD4054B2"/>
                <gradient
                    android:angle="225"
                    android:startColor="#DD4054B2"
                    android:endColor="#334054B2"/>
            </shape>
        </clip>
    </item>
</layer-list>
